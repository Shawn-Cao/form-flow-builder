  "bugs": {
    "url": "https://github.com/shawnca/form-flow-builder/issues"
  },
  "homepage": "https://github.com/shawnca/form-flow-builder#readme",
  "dependencies": {
    "js-yaml": "^4.1.0",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@types/js-yaml": "^4.0.9",
    "jest": "^29.7.0",
    "react": "^19.1.0"
  },
